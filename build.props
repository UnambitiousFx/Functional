<Project>
    <PropertyGroup>
        <Authors>Oleexo</Authors>
        <Product>UnambitiousFx</Product>
        <RootNamespace>$(Product).$(MSBuildProjectName)</RootNamespace>
        <AssemblyName>$(Product).$(MSBuildProjectName)</AssemblyName>
        <WarningsAsErrors>true</WarningsAsErrors>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <LangVersion>latest</LangVersion>
        <Copyright>Copyright 2025 (c) Maxime Charles. All rights reserved.</Copyright>
        <GoalTargetFrameworks>net8.0;net9.0;net10.0</GoalTargetFrameworks>
        <LatestTargetFramework>net10.0</LatestTargetFramework>
        <Version>1.0.0</Version>
    </PropertyGroup>


</Project>
